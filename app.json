{
  "expo": {
    "name": "Stormii",
    "slug": "stormii",
    "version": "1.0.0",
    "scheme": "stormii",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      [
        "react-native-maps",
        {
          "useGoogleMaps": true
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Esta aplicaci贸n necesita acceso a tu ubicaci贸n para mostrar d贸nde cay贸 el rayo en el mapa."
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyAwkZJnCHLRh3UdGG0r2uHGKg8SZmIPogI"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.stormii.app",
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION"],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAwkZJnCHLRh3UdGG0r2uHGKg8SZmIPogI"
        }
      }
    }
  }
}
